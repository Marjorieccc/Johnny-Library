{
    "compilerOptions": {
      /* Language and Environment */
      "target": "es2016", 
      /* Modules */
      "module": "commonjs" ,
      "outDir": "./build",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      /* Type Checking */
      "strict": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
    },
    "include": ["src/**/*"]
  }
  